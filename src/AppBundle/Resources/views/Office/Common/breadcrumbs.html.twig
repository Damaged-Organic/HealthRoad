{# AppBundle/Resources/views/Office/Common/breadcrumbs.html.twig #}
<div id="breadcrumbs">
    <ul>

        {% for breadcrumb in breadcrumbs %}
            <li><a href="{{ breadcrumb.url }}">{{ breadcrumb.title }}</a></li>

            {% if not loop.last %}
                <span class="hr-right-arrow"></span>
            {% endif %}
        {% endfor %}
    </ul>
</div>